# SuperAI 开发环境配置
# 开发环境专用配置文件
# 创建时间: 2024-09-15
# 维护者: DevOps团队

# === 环境标识 ===
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# === 核心服务端口配置 ===
AGENT_PLANNER_PORT=8300
AGENT_EXECUTOR_PORT=8400
AGI_QWEN_SERVICE_PORT=8201
VLLM_SERVICE_PORT=8000

# === Redis配置 ===
REDIS_HOST=agi-redis-lb
REDIS_PORT=6379
REDIS_PASSWORD=dev_redis_password
REDIS_DB=0
REDIS_MAX_CONNECTIONS=20

# === 监控服务配置 ===
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# === 负载均衡器配置 ===
HEALTH_CHECK_PROXY_PORT=8105
LEARNING_API_LB_PORT=8200
QWEN_LB_PORT=8202
SYNERGY_API_LB_PORT=8199

# === API密钥配置 (开发环境) ===
TAVILY_API_KEY=your_dev_tavily_api_key
OPENAI_API_KEY=your_dev_openai_api_key

# === 安全配置 (开发环境) ===
SECRET_KEY=dev_secret_key_not_for_production
JWT_SECRET=dev_jwt_secret_key
JWT_EXPIRATION=3600

# === 数据库配置 ===
DATABASE_URL=sqlite:///./superai_dev.db

# === 日志配置 ===
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=logs/superai_dev.log
LOG_MAX_SIZE=10MB
LOG_BACKUP_COUNT=5

# === 性能配置 ===
WORKER_TIMEOUT=120
WORKER_CONNECTIONS=1000
MAX_REQUESTS=1000
MAX_REQUESTS_JITTER=100

# === 开发工具配置 ===
FLASK_ENV=development
FLASK_DEBUG=true
WERKZEUG_DEBUG_PIN=off

# === 监控配置 ===
PROMETHEUS_METRICS_ENABLED=true
METRICS_PORT=8080
HEALTH_CHECK_INTERVAL=30

# === 事件总线配置 ===
EVENT_BUS_RETRY_ATTEMPTS=3
EVENT_BUS_RETRY_DELAY=1
EVENT_BUS_TIMEOUT=30

# === 工具配置 ===
TOOL_EXECUTION_TIMEOUT=60
MAX_TOOL_RETRIES=3
TOOL_CACHE_ENABLED=true

# === 开发特性开关 ===
ENABLE_DEBUG_ENDPOINTS=true
ENABLE_PROFILING=true
ENABLE_MOCK_SERVICES=false
ENABLE_AUTO_RELOAD=true